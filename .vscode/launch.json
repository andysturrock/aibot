{
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ts-node - run-local.ts",
      "runtimeExecutable": "ts-node",
      "program": "__tests__/run-local.ts",
      "cwd": "${workspaceFolder}/packages/lambda-src/",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
  ]
}